# React App Environment Variables
# Copy this file to .env.local and fill in the values

# Backend API URL (use your deployed backend URL for production)
REACT_APP_API_URL=http://localhost:5000

# Environment (development, production)
REACT_APP_ENVIRONMENT=production

# Enable/disable debug mode
REACT_APP_DEBUG=false

# MediaPipe model paths (optional, using CDN by default)
# REACT_APP_FACE_MESH_MODEL_URL=
# REACT_APP_POSE_MODEL_URL=

# Analytics and monitoring (if you add them later)
# REACT_APP_ANALYTICS_ID=
# REACT_APP_SENTRY_DSN= 